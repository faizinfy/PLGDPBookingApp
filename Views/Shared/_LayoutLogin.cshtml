
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Pusat Latihan Guru Dalam Perkhidmatan</title>
    @*@Styles.Render("~/Content/css")*@

    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="@Url.Content("~/themes")/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    
    <link rel="stylesheet" href="@Url.Content("~/themes")/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">

    <link rel="stylesheet" href="@Url.Content("~/themes")/plugins/select2/css/select2.min.css">

    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="@Url.Content("~/themes")/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/themes")/dist/css/adminlte.min.css">

    <link rel="stylesheet" href="@Url.Content("~/themes")/plugins/daterangepicker/daterangepicker.css">

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition login-page">
    @RenderBody()
    
    <!-- jQuery -->
    <script src="@Url.Content("~/themes")/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="@Url.Content("~/themes")/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- daterangepicker -->
    <script src="@Url.Content("~/themes")/plugins/moment/moment.min.js"></script>
    <script src="@Url.Content("~/themes")/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="@Url.Content("~/themes")/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>

    <script src="@Url.Content("~/themes")/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>

    <!-- AdminLTE App -->
    <script src="@Url.Content("~/themes")/dist/js/adminlte.min.js"></script>

    <script src="@Url.Content("~/themes")/plugins/select2/js/select2.full.min.js"></script>


    @RenderSection("scripts", required: false)

</body>
</html>